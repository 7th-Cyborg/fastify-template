{
  "name": "fastify-template",
  "version": "1.0.0",
  "description": "Fastify Template (Node 14+, Fastify 3+, eslint)",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "start:dev": "nodemon src/server.ts",
    "start:fastify": "npm run build && fastify start -l info dist/app.js",
    "build": "tsc -p .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "DC",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.18.0",
    "fastify-autoload": "^3.7.1",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-plugin-jest": "^24.3.6",
    "fastify-cli": "^2.13.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
